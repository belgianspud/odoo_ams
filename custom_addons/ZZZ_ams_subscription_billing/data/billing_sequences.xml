<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Billing Schedule Sequence -->
        <record id="sequence_ams_billing_schedule" model="ir.sequence">
            <field name="name">AMS Billing Schedule</field>
            <field name="code">ams.billing.schedule</field>
            <field name="prefix">BS</field>
            <field name="suffix"></field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Billing Event Sequence -->
        <record id="sequence_ams_billing_event" model="ir.sequence">
            <field name="name">AMS Billing Event</field>
            <field name="code">ams.billing.event</field>
            <field name="prefix">BE</field>
            <field name="suffix"></field>
            <field name="padding">8</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Manual Billing Event Sequence -->
        <record id="sequence_ams_manual_billing" model="ir.sequence">
            <field name="name">AMS Manual Billing</field>
            <field name="code">ams.manual.billing</field>
            <field name="prefix">MB</field>
            <field name="suffix"></field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>