<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- ========================================================================== -->
        <!-- DEFAULT SUBSCRIPTION PRODUCT DEFINITIONS -->
        <!-- ========================================================================== -->
        
        <!-- Individual Membership Subscriptions -->
        <record id="subscription_individual_monthly" model="ams.subscription.product">
            <field name="name">Individual Monthly Membership</field>
            <field name="code">INDIV_MONTHLY</field>
            <field name="subscription_type">membership</field>
            <field name="subscription_category">core</field>
            <field name="default_duration">1</field>
            <field name="duration_unit">months</field>
            <field name="base_price">25.00</field>
            <field name="sequence">10</field>
            <field name="active">True</field>
            <field name="member_only">False</field>
            <field name="short_description">Monthly individual membership with full access to association benefits.</field>
            <field name="benefits_included">• Monthly newsletter
• Member directory access
• Event discounts
• Professional resources
• Networking opportunities</field>
        </record>

        <record id="subscription_individual_quarterly" model="ams.subscription.product">
            <field name="name">Individual Quarterly Membership</field>
            <field name="code">INDIV_QUARTERLY</field>
            <field name="subscription_type">membership</field>
            <field name="subscription_category">core</field>
            <field name="default_duration">3</field>
            <field name="duration_unit">months</field>
            <field name="base_price">70.00</field>
            <field name="sequence">20</field>
            <field name="active">True</field>
            <field name="member_only">False</field>
            <field name="short_description">Quarterly individual membership with 7% savings over monthly.</field>
            <field name="benefits_included">• Quarterly newsletter bundle
• Member directory access
• Event discounts
• Professional resources
• Priority support</field>
        </record>

        <record id="subscription_individual_annual" model="ams.subscription.product">
            <field name="name">Individual Annual Membership</field>
            <field name="code">INDIV_ANNUAL</field>
            <field name="subscription_type">membership</field>
            <field name="subscription_category">core</field>
            <field name="default_duration">12</field>
            <field name="duration_unit">months</field>
            <field name="base_price">250.00</field>
            <field name="sequence">30</field>
            <field name="active">True</field>
            <field name="member_only">False</field>
            <field name="short_description">Annual individual membership with maximum savings and benefits.</field>
            <field name="benefits_included">• Full year of benefits
• Annual conference discount
• Premium member directory
• Professional certification discounts
• Exclusive member events
• Priority customer support</field>
        </record>

        <!-- Student Membership Subscriptions -->
        <record id="subscription_student_annual" model="ams.subscription.product">
            <field name="name">Student Annual Membership</field>
            <field name="code">STUDENT_ANNUAL</field>
            <field name="subscription_type">membership</field>
            <field name="subscription_category">specialty</field>
            <field name="default_duration">12</field>
            <field name="duration_unit">months</field>
            <field name="base_price">50.00</field>
            <field name="sequence">40</field>
            <field name="active">True</field>
            <field name="member_only">False</field>
            <field name="requires_approval">True</field>
            <field name="short_description">Discounted annual membership for full-time students.</field>
            <field name="benefits_included">• Student-rate event pricing
• Career development resources
• Mentorship program access
• Student networking events
• Scholarship notifications</field>
        </record>

        <!-- Corporate/Enterprise Subscriptions -->
        <record id="subscription_corporate_annual" model="ams.subscription.product">
            <field name="name">Corporate Annual Membership</field>
            <field name="code">CORP_ANNUAL</field>
            <field name="subscription_type">membership</field>
            <field name="subscription_category">premium</field>
            <field name="default_duration">12</field>
            <field name="duration_unit">months</field>
            <field name="base_price">1500.00</field>
            <field name="sequence">50</field>
            <field name="active">True</field>
            <field name="member_only">False</field>
            <field name="requires_approval">True</field>
            <field name="short_description">Comprehensive corporate membership with multi-user benefits.</field>
            <field name="benefits_included">• Up to 10 individual memberships
• Corporate directory listing
• Bulk event registration discounts
• Executive networking access
• Corporate sponsor opportunities
• Dedicated account manager</field>
        </record>

        <!-- Chapter Memberships -->
        <record id="subscription_chapter_local" model="ams.subscription.product">
            <field name="name">Local Chapter Membership</field>
            <field name="code">CHAPTER_LOCAL</field>
            <field name="subscription_type">chapter</field>
            <field name="subscription_category">add_on</field>
            <field name="default_duration">12</field>
            <field name="duration_unit">months</field>
            <field name="base_price">75.00</field>
            <field name="sequence">60</field>
            <field name="active">True</field>
            <field name="member_only">True</field>
            <field name="short_description">Annual local chapter membership for regional networking and events.</field>
            <field name="benefits_included">• Local chapter events access
• Regional networking meetings
• Local professional development
• Chapter newsletter
• Local member directory</field>
        </record>

        <!-- Publication Subscriptions -->
        <record id="subscription_journal_print" model="ams.subscription.product">
            <field name="name">Professional Journal (Print)</field>
            <field name="code">JOURNAL_PRINT</field>
            <field name="subscription_type">publication</field>
            <field name="subscription_category">add_on</field>
            <field name="default_duration">12</field>
            <field name="duration_unit">months</field>
            <field name="base_price">120.00</field>
            <field name="sequence">70</field>
            <field name="active">True</field>
            <field name="member_only">False</field>
            <field name="short_description">Annual subscription to quarterly professional journal with print delivery.</field>
            <field name="benefits_included">• 4 quarterly print issues
• Digital archive access
• Exclusive online content
• Industry research reports
• Author webinar series</field>
        </record>

        <record id="subscription_journal_digital" model="ams.subscription.product">
            <field name="name">Professional Journal (Digital)</field>
            <field name="code">JOURNAL_DIGITAL</field>
            <field name="subscription_type">publication</field>
            <field name="subscription_category">add_on</field>
            <field name="default_duration">12</field>
            <field name="duration_unit">months</field>
            <field name="base_price">80.00</field>
            <field name="sequence">80</field>
            <field name="active">True</field>
            <field name="member_only">False</field>
            <field name="short_description">Annual digital subscription to professional journal with immediate access.</field>
            <field name="benefits_included">• Immediate digital access
• Mobile-friendly reading
• Complete digital archive
• Search functionality
• Downloadable articles
• Author webinar series</field>
        </record>

        <!-- Service Subscriptions -->
        <record id="subscription_premium_support" model="ams.subscription.product">
            <field name="name">Premium Support Service</field>
            <field name="code">PREMIUM_SUPPORT</field>
            <field name="subscription_type">service</field>
            <field name="subscription_category">premium</field>
            <field name="default_duration">12</field>
            <field name="duration_unit">months</field>
            <field name="base_price">300.00</field>
            <field name="sequence">90</field>
            <field name="active">True</field>
            <field name="member_only">True</field>
            <field name="short_description">Premium support service with priority assistance and consultation.</field>
            <field name="benefits_included">• Priority phone support
• Dedicated support representative
• Monthly consultation sessions
• Industry expertise access
• Fast-track issue resolution
• Premium resource library</field>
        </record>

        <!-- Certification Program Subscriptions -->
        <record id="subscription_certification_program" model="ams.subscription.product">
            <field name="name">Certification Program Access</field>
            <field name="code">CERT_PROGRAM</field>
            <field name="subscription_type">certification</field>
            <field name="subscription_category">specialty</field>
            <field name="default_duration">24</field>
            <field name="duration_unit">months</field>
            <field name="base_price">800.00</field>
            <field name="sequence">100</field>
            <field name="active">True</field>
            <field name="member_only">True</field>
            <field name="requires_approval">True</field>
            <field name="short_description">24-month certification program with comprehensive training and assessment.</field>
            <field name="benefits_included">• Complete certification curriculum
• Online learning platform access
• Expert instructor support
• Practice exams and assessments
• Digital certificate upon completion
• Continuing education credits
• Alumni network access</field>
        </record>

        <!-- Training & Education Subscriptions -->
        <record id="subscription_webinar_series" model="ams.subscription.product">
            <field name="name">Monthly Webinar Series</field>
            <field name="code">WEBINAR_MONTHLY</field>
            <field name="subscription_type">training</field>
            <field name="subscription_category">add_on</field>
            <field name="default_duration">6</field>
            <field name="duration_unit">months</field>
            <field name="base_price">150.00</field>
            <field name="sequence">110</field>
            <field name="active">True</field>
            <field name="member_only">False</field>
            <field name="short_description">6-month series of professional development webinars.</field>
            <field name="benefits_included">• 6 expert-led webinars
• Live Q&amp;A sessions
• Downloadable resources
• Continuing education credits
• Recording access for 1 year
• Certificate of completion</field>
        </record>

        <!-- Committee Access -->
        <record id="subscription_committee_access" model="ams.subscription.product">
            <field name="name">Professional Committee Access</field>
            <field name="code">COMMITTEE_ACCESS</field>
            <field name="subscription_type">committee</field>
            <field name="subscription_category">specialty</field>
            <field name="default_duration">12</field>
            <field name="duration_unit">months</field>
            <field name="base_price">200.00</field>
            <field name="sequence">120</field>
            <field name="active">True</field>
            <field name="member_only">True</field>
            <field name="requires_approval">True</field>
            <field name="short_description">Annual access to professional committees and working groups.</field>
            <field name="benefits_included">• Committee meeting participation
• Industry standards development
• Professional networking
• Leadership opportunities
• Expert collaboration
• Committee resources access</field>
        </record>

    </data>
</odoo>