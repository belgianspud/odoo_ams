        <!-- Demo Subscription Plans -->

        <record id="demo_plan_individual_annual" model="subscription.plan">
            <field name="name">Individual Member - Annual</field>
            <field name="code">IND-ANNUAL</field>
            <field name="product_template_id" ref="demo_product_individual_membership"/>
            <field name="price">299.00</field>
            <field name="billing_period">yearly</field>
            <field name="billing_interval">1</field>
            <field name="billing_type">anniversary</field>
            <field name="trial_period">30</field>
            <field name="trial_price">0.00</field>
            <field name="auto_renew">True</field>
            <field name="description">
                <![CDATA[
                <p>Annual individual membership with full access to all member benefits.</p>
                <p>Includes 30-day free trial period.</p>
                ]]>
