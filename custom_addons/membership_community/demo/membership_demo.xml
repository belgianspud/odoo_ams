        <!-- Demo Membership Products -->

        <record id="demo_product_individual_membership" model="product.template">
            <field name="name">Individual Member - Annual</field>
            <field name="type">service</field>
            <field name="list_price">299.00</field>
            <field name="is_subscription">True</field>
            <field name="is_membership_product">True</field>
            <field name="default_member_category_id" ref="category_individual"/>
            <field name="portal_access_level">standard</field>
            <field name="enables_credentials">True</field>
            <field name="enables_ce_tracking">True</field>
            <field name="description">
                <![CDATA[
                <p>Full individual membership with access to all member benefits including:</p>
                <ul>
                    <li>Member portal access</li>
                    <li>Event discounts</li>
                    <li>Digital journal access</li>
                    <li>Networking opportunities</li>
                    <li>Professional development resources</li>
                </ul>
                ]]>
            </field>
            <field name="benefit_ids" eval="[(6, 0, [
                ref('benefit_portal_access'),
                ref('benefit_event_discount_20'),
                ref('benefit_journal_digital'),
                ref('benefit_newsletter'),
                ref('benefit_member_directory'),
                ref('benefit_directory_listing'),
                ref('benefit_webinar_library'),
                ref('benefit_voting_rights')
            ])]"/>
            <field name="feature_ids" eval="[(6, 0, [
                ref('feature_portal_access'),
                ref('feature_directory_listing'),
                ref('feature_directory_search'),
                ref('feature_event_discount'),
                ref('feature_journal_digital'),
                ref('feature_newsletter'),
                ref('feature_webinar_access'),
                ref('feature_voting_rights')
            ])]"/>
        </record>

        <record id="demo_product_student_membership" model="product.template">
            <field name="name">Student Member - Annual</field>
            <field name="type">service</field>
            <field name="list_price">99.00</field>
            <field name="is_subscription">True</field>
            <field name="is_membership_product">True</field>
            <field name="default_member_category_id" ref="category_student"/>
            <field name="portal_access_level">basic</field>
            <field name="requires_eligibility_verification">True</field>
            <field name="eligibility_criteria">
                <![CDATA[
                <p>Must be currently enrolled in an accredited educational institution.</p>
