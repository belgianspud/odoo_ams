        <!-- Discount Benefits -->
        <record id="benefit_event_discount_10" model="membership.benefit">
            <field name="name">10% Event Registration Discount</field>
            <field name="code">EVENT_DISC_10</field>
            <field name="sequence">10</field>
            <field name="category">discount</field>
            <field name="benefit_type">monetary</field>
            <field name="is_premium_benefit" eval="False"/>
            <field name="is_highlighted" eval="True"/>
            <field name="icon">fa-percent</field>
            <field name="color">2</field>
            <field name="description">Save 10% on all event registration fees including conferences, webinars, and workshops.</field>
            <field name="short_description">10% discount on event registrations</field>
            <field name="has_monetary_value" eval="True"/>
            <field name="monetary_value">50.00</field>
            <field name="quantifiable_value">Up to $50 savings per year</field>
            <field name="marketing_tagline">Save on Every Event!</field>
            <field name="display_on_website" eval="True"/>
            <field name="display_order">10</field>
        </record>

