<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Demo Partners/Members -->
        <record id="demo_member_john" model="res.partner">
            <field name="name">John Smith</field>
            <field name="email">john.smith@example.com</field>
            <field name="is_member">True</field>
            <field name="member_type_id" ref="ams_foundation.member_type_regular"/>
            <field name="member_status">active</field>
            <field name="membership_start_date" eval="(DateTime.today() - timedelta(days=100))"/>
            <field name="membership_end_date" eval="(DateTime.today() + timedelta(days=265))"/>
            <field name="professional_designation">CPA</field>
            <field name="years_experience">8</field>
            <field name="employer_name">Smith &amp; Associates</field>
            <field name="job_title">Senior Accountant</field>
            <field name="communication_preferences">email</field>
            <field name="newsletter_subscription">True</field>
            <field name="directory_listing">True</field>
        </record>

        <record id="demo_member_jane" model="res.partner">
            <field name="name">Jane Williams</field>
            <field name="email">jane.williams@example.com</field>
            <field name="is_member">True</field>
            <field name="member_type_id" ref="ams_foundation.member_type_student"/>
            <field name="member_status">active</field>
            <field name="membership_start_date" eval="(DateTime.today() - timedelta(days=50))"/>
            <field name="membership_end_date" eval="(DateTime.today() + timedelta(days=315))"/>
            <field name="years_experience">0</field>
            <field name="employer_name">University of Excellence</field>
            <field name="job_title">Graduate Student</field>
            <field name="communication_preferences">email</field>
            <field name="newsletter_subscription">True</field>
            <field name="directory_listing">True</field>
        </record>

        <record id="demo_member_acme" model="res.partner">
            <field name="name">ACME Corporation</field>
            <field name="email">membership@acme-corp.com</field>
            <field name="is_company">True</field>
            <field name="is_member">True</field>
            <field name="member_type_id" ref="ams_foundation.member_type_corporate"/>
            <field name="member_status">active</field>
            <field name="membership_start_date" eval="(DateTime.today() - timedelta(days=200))"/>
            <field name="membership_end_date" eval="(DateTime.today() + timedelta(days=165))"/>
            <field name="communication_preferences">email</field>
            <field name="newsletter_subscription">True</field>
            <field name="directory_listing">True</field>
        </record>

        <!-- Demo Memberships -->
        <record id="demo_membership_john" model="ams.membership">
            <field name="partner_id" ref="demo_member_john"/>
            <field name="product_id" ref="product_membership_regular"/>
            <field name="start_date" eval="(DateTime.today() - timedelta(days=100))"/>
            <field name="end_date" eval="(DateTime.today() + timedelta(days=265))"/>
            <field name="membership_fee">250.00</field>
            <field name="state">active</field>
            <field name="payment_status">paid</field>
            <field name="auto_renew">True</field>
            <field name="renewal_interval">annual</field>
            <field name="notes">Demo membership for regular member</field>
        </record>

        <record id="demo_membership_jane" model="ams.membership">
            <field name="partner_id" ref="demo_member_jane"/>
            <field name="product_id" ref="product_membership_student"/>
            <field name="start_date" eval="(DateTime.today() - timedelta(days=50))"/>
            <field name="end_date" eval="(DateTime.today() + timedelta(days=315))"/>
            <field name="membership_fee">50.00</field>
            <field name="state">active</field>
            <field name="payment_status">paid</field>
            <field name="auto_renew">False</field>
            <field name="renewal_interval">annual</field>
            <field name="notes">Demo membership for student member</field>
        </record>

        <record id="demo_membership_acme" model="ams.membership">
            <field name="partner_id" ref="demo_member_acme"/>
            <field name="product_id" ref="product_membership_corporate"/>
            <field name="start_date" eval="(DateTime.today() - timedelta(days=200))"/>
            <field name="end_date" eval="(DateTime.today() + timedelta(days=165))"/>
            <field name="membership_fee">1500.00</field>
            <field name="state">active</field>
            <field name="payment_status">paid</field>
            <field name="auto_renew">True</field>
            <field name="renewal_interval">annual</field>
            <field name="notes">Demo membership for corporate member</field>
        </record>

        <!-- Demo Subscriptions -->
        <record id="demo_subscription_john_journal" model="ams.subscription">
            <field name="partner_id" ref="demo_member_john"/>
            <field name="product_id" ref="product_publication_journal"/>
            <field name="start_date" eval="(DateTime.today() - timedelta(days=80))"/>
            <field name="end_date" eval="(DateTime.today() + timedelta(days=285))"/>
            <field name="subscription_fee">120.00</field>
            <field name="state">active</field>
            <field name="payment_status">paid</field>
            <field name="auto_renew">True</field>
            <field name="renewal_interval">annual</field>
            <field name="digital_access">True</field>
            <field name="print_delivery">False</field>
            <field name="notes">Demo digital journal subscription</field>
        </record>

        <record id="demo_subscription_jane_chapter" model="ams.subscription">
            <field name="partner_id" ref="demo_member_jane"/>
            <field name="product_id" ref="product_chapter_local"/>
            <field name="start_date" eval="(DateTime.today() - timedelta(days=30))"/>
            <field name="end_date" eval="(DateTime.today() + timedelta(days=335))"/>
            <field name="subscription_fee">75.00</field>
            <field name="state">active</field>
            <field name="payment_status">paid</field>
            <field name="auto_renew">True</field>
            <field name="renewal_interval">annual</field>
            <field name="chapter_role">member</field>
            <field name="notes">Demo local chapter subscription</field>
        </record>

        <record id="demo_subscription_acme_journal_print" model="ams.subscription">
            <field name="partner_id" ref="demo_member_acme"/>
            <field name="product_id" ref="product_publication_journal_print"/>
            <field name="start_date" eval="(DateTime.today() - timedelta(days=150))"/>
            <field name="end_date" eval="(DateTime.today() + timedelta(days=215))"/>
            <field name="subscription_fee">180.00</field>
            <field name="state">active</field>
            <field name="payment_status">paid</field>
            <field name="auto_renew">True</field>
            <field name="renewal_interval">annual</field>
            <field name="digital_access">True</field>
            <field name="print_delivery">True</field>
            <field name="delivery_address_id" ref="demo_member_acme"/>
            <field name="notes">Demo digital + print journal subscription</field>
        </record>

        <!-- Demo Renewals -->
        <record id="demo_renewal_john" model="ams.renewal">
            <field name="membership_id" ref="demo_membership_john"/>
            <field name="renewal_date" eval="(DateTime.today() - timedelta(days=30))"/>
            <field name="new_end_date" eval="(DateTime.today() + timedelta(days=335))"/>
            <field name="amount">250.00</field>
            <field name="original_amount">250.00</field>
            <field name="renewal_type">early</field>
            <field name="renewal_period">annual</field>
            <field name="state">confirmed</field>
            <field name="renewal_reason">Early renewal with discount</field>
        </record>

        <!-- Demo Benefit Usage -->
        <record id="demo_benefit_usage_1" model="ams.benefit.usage">
            <field name="benefit_id" ref="benefit_event_discount"/>
            <field name="partner_id" ref="demo_member_john"/>
            <field name="quantity">1</field>
            <field name="usage_date" eval="(DateTime.today() - timedelta(days=10))"/>
            <field name="notes">Used member discount for annual conference</field>
        </record>

        <record id="demo_benefit_usage_2" model="ams.benefit.usage">
            <field name="benefit_id" ref="benefit_portal_access"/>
            <field name="partner_id" ref="demo_member_jane"/>
            <field name="quantity">1</field>
            <field name="usage_date" eval="(DateTime.today() - timedelta(days=5))"/>
            <field name="notes">Portal access login</field>
        </record>

    </data>
</odoo>