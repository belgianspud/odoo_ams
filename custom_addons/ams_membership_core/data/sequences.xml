        <record id="seq_ams_mass_renewal" model="ir.sequence">
            <field name="name">AMS Mass Renewal Batch</field>
            <field name="code">ams.mass.renewal</field>
            <field name="prefix">MRB</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="active">True</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>