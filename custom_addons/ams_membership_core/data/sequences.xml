        <record id="seq_ams_subscription_chapter" model="ir.sequence">
            <field name="name">AMS Chapter Subscription</field>
            <field name="code">ams.subscription.chapter</field>
            <field name="prefix">CHT</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="active">True</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Renewal Sequence -->
        <record id="seq_ams_renewal" model="ir.sequence">
            <field name="name">AMS Renewal Sequence</field>
            <field name="code">ams.renewal</field>
            <field name="prefix">REN</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="active">True</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Benefit Sequence -->
