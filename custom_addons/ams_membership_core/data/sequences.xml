<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Membership Sequence -->
        <record id="seq_ams_membership" model="ir.sequence">
            <field name="name">AMS Membership Sequence</field>
            <field name="code">ams.membership</field>
            <field name="prefix">MEM</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="active">True</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Subscription Sequence -->
        <record id="seq_ams_subscription" model="ir.sequence">
            <field name="name">AMS Subscription Sequence</field>
            <field name="code">ams.subscription</field>
            <field name="prefix">SUB</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="active">True</field>
            <field name="company_id" eval="False"/>
        </record>

