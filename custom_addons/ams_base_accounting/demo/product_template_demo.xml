<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Demo membership products -->
        <record id="product_individual_membership" model="product.template">
            <field name="name">Individual Annual Membership</field>
            <field name="list_price">150.00</field>
            <field name="type">service</field>
            <field name="is_subscription_product" eval="True"/>
            <field name="use_ams_accounting" eval="True"/>
            <field name="ams_product_type">individual</field>
            <field name="subscription_period">annual</field>
            <field name="categ_id" ref="product_category_ams_individual"/>
        </record>
        
        <record id="product_corporate_membership" model="product.template">
            <field name="name">Corporate Annual Membership</field>
            <field name="list_price">1200.00</field>
            <field name="type">service</field>
            <field name="is_subscription_product" eval="True"/>
            <field name="use_ams_accounting" eval="True"/>
            <field name="ams_product_type">enterprise</field>
            <field name="subscription_period">annual</field>
            <field name="categ_id" ref="product_category_ams_corporate"/>
        </record>
    </data>
</odoo>