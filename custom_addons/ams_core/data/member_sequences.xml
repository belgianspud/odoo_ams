<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Member ID Sequence -->
        <record id="seq_member_id" model="ir.sequence">
            <field name="name">Member ID Sequence</field>
            <field name="code">ams.member.id</field>
            <field name="prefix">MEM</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="False"/>
            <field name="implementation">standard</field>
        </record>

        <!-- Contact Role Assignment Sequence -->
        <record id="seq_contact_role_assignment" model="ir.sequence">
            <field name="name">Contact Role Assignment Sequence</field>
            <field name="code">ams.contact.role.assignment</field>
            <field name="prefix">CRA</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="False"/>
            <field name="implementation">standard</field>
        </record>

        <!-- Communication Preference Sequence -->
        <record id="seq_communication_preference" model="ir.sequence">
            <field name="name">Communication Preference Sequence</field>
            <field name="code">ams.communication.preference</field>
            <field name="prefix">CP</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="False"/>
            <field name="implementation">standard</field>
        </record>

        <!-- Audit Log Sequence -->
        <record id="seq_audit_log" model="ir.sequence">
            <field name="name">Audit Log Sequence</field>
            <field name="code">ams.audit.log</field>
            <field name="prefix">AL</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="False"/>
            <field name="implementation">standard</field>
        </record>

        <!-- Alternative Member ID Sequences for Different Membership Types -->
        
        <!-- Individual Member Sequence -->
        <record id="seq_member_id_individual" model="ir.sequence">
            <field name="name">Individual Member ID Sequence</field>
            <field name="code">ams.member.id.individual</field>
            <field name="prefix">IND</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="False"/>
            <field name="implementation">standard</field>
            <field name="active" eval="False"/>
        </record>

        <!-- Corporate Member Sequence -->
        <record id="seq_member_id_corporate" model="ir.sequence">
            <field name="name">Corporate Member ID Sequence</field>
            <field name="code">ams.member.id.corporate</field>
            <field name="prefix">CORP</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="False"/>
            <field name="implementation">standard</field>
            <field name="active" eval="False"/>
        </record>

        <!-- Student Member Sequence -->
        <record id="seq_member_id_student" model="ir.sequence">
            <field name="name">Student Member ID Sequence</field>
            <field name="code">ams.member.id.student</field>
            <field name="prefix">STU</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="False"/>
            <field name="implementation">standard</field>
            <field name="active" eval="False"/>
        </record>

        <!-- Lifetime Member Sequence -->
        <record id="seq_member_id_lifetime" model="ir.sequence">
            <field name="name">Lifetime Member ID Sequence</field>
            <field name="code">ams.member.id.lifetime</field>
            <field name="prefix">LIFE</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="False"/>
            <field name="implementation">standard</field>
            <field name="active" eval="False"/>
        </record>

        <!-- Year-based Member ID Sequence (if needed) -->
        <record id="seq_member_id_yearly" model="ir.sequence">
            <field name="name">Yearly Member ID Sequence</field>
            <field name="code">ams.member.id.yearly</field>
            <field name="prefix">%(year)s-MEM</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="True"/>
            <field name="implementation">standard</field>
            <field name="active" eval="False"/>
        </record>

        <!-- Date range for yearly sequence (example for current year) -->
        <record id="seq_member_id_yearly_range_2024" model="ir.sequence.date_range">
            <field name="sequence_id" ref="seq_member_id_yearly"/>
            <field name="date_from">2024-01-01</field>
            <field name="date_to">2024-12-31</field>
            <field name="number_next">1</field>
        </record>

    </data>
</odoo>