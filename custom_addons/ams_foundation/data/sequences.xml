<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Member Number Sequence -->
        <record id="seq_member_number" model="ir.sequence">
            <field name="name">Member Number Sequence</field>
            <field name="code">ams.member.number</field>
            <field name="prefix">M</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="active">True</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Engagement Rule Sequence -->
        <record id="seq_engagement_rule" model="ir.sequence">
            <field name="name">Engagement Rule Sequence</field>
            <field name="code">ams.engagement.rule</field>
            <field name="prefix">ER</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="active">True</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>