<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Cron Job for Processing Scheduled Push Campaigns -->
        <record id="ir_cron_process_scheduled_push_campaigns" model="ir.cron">
            <field name="name">Process Scheduled Push Campaigns</field>
            <field name="model_id" ref="model_push_notification_campaign"/>
            <field name="state">code</field>
            <field name="code">model.process_scheduled_campaigns()</field>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
            <field name="active" eval="True"/>
        </record>

    </data>
</odoo>