<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Billing Schedule Sequence -->
        <record id="sequence_ams_billing_schedule" model="ir.sequence">
            <field name="name">AMS Billing Schedule</field>
            <field name="code">ams.billing.schedule</field>
            <field name="prefix">BS</field>
            <field name="suffix"></field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Billing Event Sequence -->
        <record id="sequence_ams_billing_event" model="ir.sequence">
            <field name="name">AMS Billing Event</field>
            <field name="code">ams.billing.event</field>
            <field name="prefix">BE</field>
            <field name="suffix"></field>
            <field name="padding">8</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Payment Retry Sequence -->
        <record id="sequence_ams_payment_retry" model="ir.sequence">
            <field name="name">AMS Payment Retry</field>
            <field name="code">ams.payment.retry</field>
            <field name="prefix">PR</field>
            <field name="suffix"></field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Dunning Process Sequence -->
        <record id="sequence_ams_dunning_process" model="ir.sequence">
            <field name="name">AMS Dunning Process</field>
            <field name="code">ams.dunning.process</field>
            <field name="prefix">DP</field>
            <field name="suffix"></field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Proration Calculation Sequence -->
        <record id="sequence_ams_proration_calculation" model="ir.sequence">
            <field name="name">AMS Proration Calculation</field>
            <field name="code">ams.proration.calculation</field>
            <field name="prefix">PC</field>
            <field name="suffix"></field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Billing Run Sequence -->
        <record id="sequence_ams_billing_run" model="ir.sequence">
            <field name="name">AMS Billing Run</field>
            <field name="code">ams.billing.run</field>
            <field name="prefix">BR%(y)s%(month)s</field>
            <field name="suffix"></field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Transaction Reference Sequence for Payments -->
        <record id="sequence_ams_payment_transaction" model="ir.sequence">
            <field name="name">AMS Payment Transaction</field>
            <field name="code">ams.payment.transaction</field>
            <field name="prefix">TXN%(year)s</field>
            <field name="suffix"></field>
            <field name="padding">8</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Invoice Reference Sequence for Proration Adjustments -->
        <record id="sequence_ams_proration_invoice" model="ir.sequence">
            <field name="name">AMS Proration Invoice</field>
            <field name="code">ams.proration.invoice</field>
            <field name="prefix">PROADJ</field>
            <field name="suffix"></field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Credit Note Reference Sequence for Proration Credits -->
        <record id="sequence_ams_proration_credit" model="ir.sequence">
            <field name="name">AMS Proration Credit</field>
            <field name="code">ams.proration.credit</field>
            <field name="prefix">PROCR</field>
            <field name="suffix"></field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>